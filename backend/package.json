    "puppeteer": "^24.22.0"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4"
  }
}
